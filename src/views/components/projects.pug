include Entry
include comments

//- Project
//-



//- Project Entry
//- @param {Project} proj
mixin ProjectEntry(proj)
  +Entry.Project-Entry
    if proj.image
      +EntryImage(proj.image.src, proj.image.alt, "100%", 180)
    +EntryHeader
      +EntryTitle(proj.title)
      +EntrySubtitle(proj.type)
    +Comment(proj.description)
    //- +EntryDescription(proj.description)
    
