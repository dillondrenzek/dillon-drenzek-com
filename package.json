{
  "name": "drenzek-dillon-angular",
  "version": "5.0.0-alpha.0",
  "description": "The Portfolio Website of Dillon Drenzek, built with Angular",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dillondrenzek/drenzek-dillon-angular.git"
  },
  "engines": {
    "node": ">=7.8.0",
    "npm": ">=4.2.0"
  },
  "author": "Dillon Drenzek <dillon@drenzek.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dillondrenzek/drenzek-dillon-angular/issues"
  },
  "homepage": "https://github.com/dillondrenzek/drenzek-dillon-angular",
  "scripts": {
    "start:dev:client": "concurrently \"npm run build:client:watch\" \"npm run serve:bs\"",
    "start:dev": "npm run build:server && concurrently \"npm run build:client\" \"npm run serve:node\"",
    "start:dev:watch": "npm run build:server && concurrently \"npm run build:client:watch\" \"npm run serve:node\"",
    "start:prod": "npm install && npm run build:server && npm run build:client && npm run serve:node",
    "build:server": "tsc -p ./tsconfig.json",
    "build:client": "tsc -p src/app/tsconfig.json",
    "build:client:watch": "tsc -p src/app/ -w tsconfig.json",
    "aot": "ngc -p tsconfig-aot.json",
    "rollup": "rollup -c rollup-config.js",
    "prebuild:aot": "npm run clean:aot",
    "build:aot": "npm run aot && npm run rollup",
    "serve:node": "node server.js",
    "serve:bs": "lite-server -c=bs-config.json",
    "clean:aot": "rm -rf built/aot",
    "start": "npm run start:dev",
    "pretest": "npm run build:client",
    "test": "jasmine",
    "lint": "tslint ./src/**/*.ts -t verbose"
  },
  "dependencies": {
    "@angular/common": "~4.0.1",
    "@angular/compiler": "~4.0.1",
    "@angular/compiler-cli": "^4.0.1",
    "@angular/core": "~4.0.1",
    "@angular/forms": "~4.0.1",
    "@angular/http": "~4.0.1",
    "@angular/platform-browser": "~4.0.1",
    "@angular/platform-browser-dynamic": "~4.0.1",
    "@angular/platform-server": "^4.0.1",
    "@angular/router": "~4.0.1",
    "@types/express": "^4.0.35",
    "@types/jasmine": "2.5.36",
    "@types/node": "^6.0.46",
    "angular-in-memory-web-api": "~0.3.0",
    "canonical-path": "0.0.2",
    "concurrently": "^3.2.0",
    "core-js": "^2.4.1",
    "express": "^4.15.2",
    "jasmine": "^2.5.3",
    "jasmine-core": "~2.4.1",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "path": "^0.12.7",
    "rimraf": "^2.5.4",
    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "tslint": "^3.15.1",
    "typescript": "~2.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.1"
  }
}
