{
  "name": "drenzek-dillon-angular",
  "version": "5.0.0-alpha.0",
  "description": "The Portfolio Website of Dillon Drenzek, built with Angular",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dillondrenzek/drenzek-dillon-angular.git"
  },
  "engines" : {
    "node" : ">=4.2.6",
    "npm" : ">=2.14.12"
  },
  "author": "Dillon Drenzek <dillon@drenzek.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dillondrenzek/drenzek-dillon-angular/issues"
  },
  "homepage": "https://github.com/dillondrenzek/drenzek-dillon-angular",
  "scripts": {
    "preinstall": "rm -rf node_modules/",
    "start:dev:client": "concurrently \"npm run build:watch\" \"npm run serve:bs\"",
    "start:dev": "npm run build:server && concurrently \"npm run build:client\" \"npm run serve:node\"",
    "start:dev:watch": "npm run build:server && concurrently \"npm run build:client:watch\" \"npm run serve:node\"",
    "start:prod": "npm install && npm run build:server && npm run build:client && npm run serve:node",
    "build:server": "tsc -p ./tsconfig.json",
    "build:client": "tsc -p src/app/",
    "build:client:watch": "tsc -p src/app/ -w",
    "build:e2e": "tsc -p e2e/",
    "serve:node": "node server.js",
    "serve:bs": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "start": "npm run start:dev",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose"
  },
  "dependencies": {
    "@angular/common": "~4.0.0",
    "@angular/compiler": "~4.0.0",
    "@angular/core": "~4.0.0",
    "@angular/forms": "~4.0.0",
    "@angular/http": "~4.0.0",
    "@angular/platform-browser": "~4.0.0",
    "@angular/platform-browser-dynamic": "~4.0.0",
    "@angular/router": "~4.0.0",
    "angular-in-memory-web-api": "~0.3.0",
    "core-js": "^2.4.1",
    "express": "^4.15.2",
    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "typescript": "~2.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/jasmine": "2.5.36",
    "@types/node": "^6.0.46",
    "canonical-path": "0.0.2",
    "concurrently": "^3.2.0",
    "jasmine-core": "~2.4.1",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "rimraf": "^2.5.4",
    "tslint": "^3.15.1"
  }
}
